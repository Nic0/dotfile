URxvt.geometry:              80x25
URxvt.background:            #000000
URxvt.foreground:            #ffffff
URxvt.inheritPixmap:         true
URxvt.reverseVideo:          false
URxvt.tintColor:             #ffffff
URxvt.shading:               40            
#!trans=100, 0=opaque
URxvt.fading:                0              
#!term unfocus
!URxvt.font:                  xft:inconsolata:pixelsize=16:antialias=false
URxvt.font:                 xft:Bitstream Vera Sans Mono:pixelsize=15:antialias=false 
URxvt.boldFont:		     9x15bold,xft:Bitstream Vera Sans Mono:pixelsize=15:antialias=false
URxvt.scrollBar:             false
URxvt.scrollBar_floating:    true
URxvt.scrollBar_right:       true
URxvt.scrollColor:           black
URxvt.scrollstyle:           xterm
URxvt.saveLines:             10000
URxvt.cursorColor:           #7300FF
URxvt.cursorBlink:           false
URxvt.colorBD:		     white
#Pour ne pas avoir de bordures
URxvt*borderLess:            true 

Urxvt*background: rgba:0000/0000/0000/cccc
Urxvt*depth: 32

URxvt.perl-ext:			default,matcher
URxvt.urlLauncher:		/usr/bin/firefox
URxvt.mather.button:		2	


